{% extends 'LSMainBundle::layout.html.twig' %}

{% block body %}
    {% for user in users %}
        <p>----------</p>
        <div>
        <p>{{user.id}}</p>
        <p>{{user.username}}</p>
        <p>{{user.password}}</p>
        <p>{{user.email}}</p> 
        <div class="date"><time datetime="{{ user.created|date('c') }}">{{ user.created|date('l, F j, Y') }}</time></div>
        </div>
        
    {% else %}
        <p>There are no users right now</p>
    {% endfor %}
{% endblock %}